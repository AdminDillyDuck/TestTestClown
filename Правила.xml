			Объект.Склад = Параметры.СкладДляОбменаДаннымиСУТ;
			
		КонецЕсли;
		
	КонецЕсли;

	// Заполение реквизита ПодразделениеОрганизации
	Если ТипЗнч(Объект) = Тип("ДокументОбъект.СписаниеСРасчетногоСчета")
